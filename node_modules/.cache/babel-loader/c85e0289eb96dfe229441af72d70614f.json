es":[],"mappings":";;;;;;;;AACsB,EAAA,KAAK,EAAC;;;;AACpB,EAAA,KAAK,EAAC;;;;AAKL,EAAA,KAAK,EAAC;;;AAEN,EAAA,KAAK,EAAC;;;AACN,EAAA,KAAK,EAAC;;;;AACD,EAAA,KAAK,EAAC;;;;AACN,EAAA,KAAK,EAAC,K;AAAa,EAAA,QAAQ,EAAR;;;;;;;;SAXpB,KAAA,CAAA,O,kBAAX,mBAAA,CAaM,KAbN,EAAA,UAAA,EAaM,CAZoB,KAAA,CAAA,OAAA,CAAQ,K,kBAAhC,mBAAA,CAIK,IAJL,EAAA,UAAA,EAIK,E,kBAHH,mBAAA,CAEK,SAFL,EAEK,IAFL,EAEK,WAAA,CAFuB,KAAA,CAAA,OAAA,CAAQ,KAE/B,EAFoC,UAA7B,IAA6B,EAAvB,KAAuB,EAAlB;yBAAvB,mBAAA,CAEK,IAFL,EAEK;AAFuC,MAAA,GAAG,EAAE;AAE5C,KAFL,EAAsD,CACpD,mBAAA,CAA0C,KAA1C,EAA0C;AAApC,MAAA,GAAG,EAAE,IAAI,CAAC,GAA0B;AAApB,MAAA,GAAG,EAAE,IAAI,CAAC;AAAU,KAA1C,E,IAAA,E;;AAAA,M,UAAA,CADoD,CAAtD,C;GAEK,CAFL,E;;AAAA,GAGG,EAJL,C,qCAYI,EAPJ,mBAAA,CAMM,KANN,EAAA,UAAA,EAMM,CALJ,mBAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,KAAA,CAAA,OAAA,CAAQ,IAAY,CAA3B,EAAmB;AAAA;AAAnB,GAKI,EAJJ,mBAAA,CAAsC,GAAtC,EAAA,UAAA,EAAsC,gBAAA,CAAlB,KAAA,CAAA,WAAkB,CAAtC,EAA+B;AAAA;AAA/B,GAII,EAHJ,mBAAA,CAAgD,GAAhD,EAAA,UAAA,EAAgD,gBAAA,CAAxB,KAAA,CAAA,OAAA,CAAQ,SAAgB,CAAhD,EAAyC;AAAA;AAAzC,GAGI,EAFsB,KAAA,CAAA,OAAA,CAAQ,OAAR,KAAe,O,kBAAzC,mBAAA,CAAuE,QAAvE,EAAA,UAAA,EAAuD,SAAvD,C,mBACA,mBAAA,CAA4D,QAA5D,EAAA,UAAA,EAAoC,iBAApC,C,CACI,CANN,CAOI,CAbN,C,mBAeA,mBAAA,CAEM,KAFN,EAEM,UAFN,EAEM,CADJ,YAAA,CAAoB,2BAApB,CACI,CAFN,C","sourcesContent":["<template>\r\n  <div v-if=\"produto\" class=\"produto\">\r\n    <ul class=\"fotos\" v-if=\"produto.fotos\">\r\n      <li v-for=\"(foto, index) in produto.fotos\" :key=\"index\">\r\n        <img :src=\"foto.src\" :alt=\"foto.titulo\" />\r\n      </li>\r\n    </ul>\r\n    <div class=\"info\">\r\n      <h1>{{ produto.nome }}</h1>\r\n      <p class=\"preco\">{{ numberPreco }}</p>\r\n      <p class=\"descricao\">{{ produto.descricao }}</p>\r\n      <button class=\"btn\" v-if=\"produto.vendido === 'false'\">Comprar</button>\r\n      <button class=\"btn\" v-else disabled>Produto Vendido</button>\r\n    </div>\r\n  </div>\r\n\r\n  <div v-else>\r\n    <PaginaCarregando />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { api } from \"@/services.js\";\r\nimport PaginaCarregando from \"@/components/PaginaCarregando.vue\";\r\n\r\nexport default {\r\n  name: \"Produtos\",\r\n  props: [\"id\"],\r\n  components: {\r\n    PaginaCarregando,\r\n  },\r\n  data() {\r\n    return {\r\n      produto: null,\r\n      numberPreco: null,\r\n    };\r\n  },\r\n  methods: {\r\n    getProduto() {\r\n      setTimeout(() => {\r\n        api.get(`/produto/${this.id}`).then((r) => {\r\n          this.produto = r.data;\r\n          var number = Number(this.produto.preco);\r\n          number = number.toLocaleString(\"pt-BR\", {\r\n            style: \"currency\",\r\n            currency: \"BRL\",\r\n          });\r\n          this.numberPreco = number;\r\n        });\r\n      }, 1500);\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getProduto();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.produto {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  grid-gap: 30px;\r\n  max-width: 900px;\r\n  padding: 60px 20px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.preco {\r\n  color: #e80;\r\n  font-weight: bold;\r\n  font-size: 1.5rem;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.descricao {\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.btn {\r\n  margin-top: 60px;\r\n  width: 200px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}